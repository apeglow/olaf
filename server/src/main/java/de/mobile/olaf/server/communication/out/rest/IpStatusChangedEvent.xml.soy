{namespace de.mobile.olaf.api.rest}


/**
 *
 * Generate xml for the REST request.
 *
 * @param events
 *
 */
{template .xml}
	<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
	<olaf:ipStatusChangedEvents xmlns:olaf="http://www.mobile.de/olaf/api/rest/namespace">
		{foreach $event in $events}
			<olaf:newStatus olaf:address="{$event.address}" olaf:status="{$event.status}" />
		{/foreach}
	</olaf:ipStatusChangedEvents>
{/template}